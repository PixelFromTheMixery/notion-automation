{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Main File",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/src/main.py",
			"console": "integratedTerminal"
		},
		{
			"name": "Database match",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/src/main.py",
			"console": "integratedTerminal",
			"args": ["-dbm"]
		},
		{
			"name": "Move tasks",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/src/main.py",
			"console": "integratedTerminal",
			"args": ["-m"]
		},
		{
			"name": "Clockify",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/src/main.py",
			"console": "integratedTerminal",
			"args": ["-c"]
		},
		{
			"name": "Setup",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/src/main.py",
			"console": "integratedTerminal",
			"args": ["-s", "clockify"]
		}
		{
			"name": "Testing",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/src/main.py",
			"console": "integratedTerminal",
			"args": ["-t"]
		}
		{
			"name": "Sync and Move",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/src/main.py",
			"console": "integratedTerminal",
			"args": ["--m", "--c"],
		}
		
	]
}
